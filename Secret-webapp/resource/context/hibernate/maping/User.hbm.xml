<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mimi.model">
	<class name="User" table="T_USER" lazy="false">
		<id name="userId" type="java.lang.String" column="userId">
		</id>
		<property name="alias" column="alias" type="java.lang.String" />
		<property name="password" column="password" type="java.lang.String" />
		<property name="imei" column="imei" type="java.lang.String" />
		<property name="status" column="status" type="java.lang.String" />
		<property name="createTime" column="createTime" type="java.lang.String" />
	</class>
</hibernate-mapping>