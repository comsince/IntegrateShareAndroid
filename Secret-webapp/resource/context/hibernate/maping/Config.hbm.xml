<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mimi.model">
	<class name="Config" table="T_CONFIG" lazy="false">
		<id name="sequenceId" type="java.lang.String" column="sequenceId">
		</id>
		<property name="key" column="ikey" type="java.lang.String" />
		<property name="value" column="value" type="java.lang.String" />
		<property name="domain" column="domain" type="java.lang.String" />
		<property name="description" column="description" type="java.lang.String" />
	</class>
</hibernate-mapping>