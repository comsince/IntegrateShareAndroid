html{overflow-y:scroll;}
html,body{height:100%;}
img{-ms-interpolation-mode:bicubic}
section,article,aside,header,footer,nav,figure{display:block}
body,h1,h2,h3,h4,h5,h6,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td,figure{margin:0;padding:0}
body,button,input,select,textarea{font:12px/1.5 arial}
h1,h2,h3,h4,h5,h6,button,input,select,textarea{font-size:100%}
address,cite,dfn,em,var{font-style:normal}
code,kbd,pre,samp{font-family:courier new,courier,monospace}
small{font-size:12px}
ul,ol{list-style:none}
a{text-decoration:none}
a:hover{text-decoration:underline}
sup{vertical-align:text-top}
sub{vertical-align:text-bottom}
legend{color:#000}
fieldset,img{border:0}
table{border-collapse:collapse;border-spacing:0}
input::-moz-focus-inner,button::-moz-focus-inner{border:0;padding:0;outline:0}
::-webkit-file-upload-button{cursor:pointer;}
.clear{*zoom:1}
.clear:after{content:'\20';display:block;clear:both;height:0}
body .hide,.app-web .hide,.app-qplus .hide,.app-appbox .hide{display:none}
body .ellipsis{overflow:hidden;-o-text-overflow:ellipsis;text-overflow:ellipsis;white-space:nowrap;_width:100%;word-wrap:normal}
body{color:#666;background:#F1F1F1}
a{color:#1D719E}
.wrapper{width:980px;margin:0 auto}
.mb{margin-bottom:15px}
.float_right{float:right}
.text_right{text-align:right}
.margin_left_15{margin-left:15px}
.margin_lr_5{margin:0 5px}
.ui-bar{color:#B1B1B1;font-style:normal;}
.ui-bold{font-weight:600;}
.ui-normal{font-weight:500;}
.ui-checkbox{vertical-align:middle;}
a.unclickable,.unclickable a{cursor:default;}
.ui-pos{position:relative;}
.ui-pos-right{position:absolute;right:0;top:0;}
.copy{position:relative}
.clipboardswf{position:absolute;left:0;top:0}
.icon-times{padding:0 9px;font:600 14px/27px Tahoma;}
 /* }}} */
a {outline: none;}
a:active {star:expression(this.onFocus=this.blur());}
/* 更好看的fosus状态 - bootstrap */
a:focus {
    outline: thin dotted #333;
    outline: 5px auto -webkit-focus-ring-color;
    outline-offset: -2px;
}
/*  IE6 固定样式 */
* html,* html body{/* 修正IE6振动bug */
    _background-image:url(about:blank);
    _background-attachment:fixed;
}

/* solution */
.sl-mid{display:table-cell;vertical-align:middle}
.sl-mid-inner{*display:inline-block;vertical-align:middle;*display:inline;*zoom:1}
.sl-mid-ie{display:none;*display:inline-block;vertical-align:middle;width:0;height:100%}
.sl-empty{width:980px;height:340px;text-align:center;}
.app-appbox .sl-empty{height:420px;width:750px;}
.app-qplus .sl-empty{height:550px;}

/* header */
.header{
	position: relative;
    height:50px;
    margin:0 auto;
    background:url(../img/top-bg.png) repeat-x left top;
    box-shadow: 0 1px 5px #ccc;
    z-index: 10;
}
.app-appbox .header{
	background:#212225;
	 box-shadow:none;
}
.header-fixed{
    position:fixed;
    top:0;
    left:0;
    width:100%;
	min-width:950px;
    *min-width:933px;
    z-index:4;
    background:#F1F1F1;
    _position:absolute;
    _top:expression(eval(document.documentElement.scrollTop));
    zoom:1;
    cursor:default;
}
.header-fixed .box_mod_maintools{
    padding-bottom:0;
}
.header-att{
	position:relative;
    width:980px;
	margin:10px auto 0;
	*zoom:1;
}
.header-cnt{
    position:relative;
    *z-index:1;
    *zoom:1;
}
.header-cnt h1{
    padding-top:10px;
}
.logo{
    position:relative;
    display:block;
	width:110px;
	height:30px;
    background:url(../img/logo-110x30.png?v20130812) 0 0 no-repeat;
    _cursor:pointer;
}
.logo span{
    position:absolute;
    top:-100em;
}
.nav{
    position:absolute;
    right:0;
    top:17px;
    font:500 12px Tahoma,'\5B8B\4F53','\5FAE\8F6F\96C5\9ED1','\534E\6587\7EC6\9ED1';
    color:#ABABAB;
    text-shadow:0 -1px 1px #000;
} 
/* appbox webkit滚动条 */
::-webkit-scrollbar-track{
    border-radius:4px;
}
::-webkit-scrollbar-track-piece{
    -webkit-border-radius:4px;
}
::-webkit-scrollbar-track-piece:hover{

}
::-webkit-scrollbar{
    width:10px;
    height:10px;
    border-radius:0px;
    background-color:#F1F1F1;
}
::-webkit-scrollbar:hover{
    background:#DCE1E4;
}
::-webkit-scrollbar-thumb{
    background-color:#C1C8CC;
    border:solid 1px #B5BBBF;
    border-radius:4px;
}
::-webkit-scrollbar-thumb:hover{
    background-color:#6C7073;
    border-color:#5F6366;
}
::-webkit-scrollbar-arrow {
    color:#F00;
    background:#0F0;
}
::-webkit-scrollbar-button{
    display:block;
    width:10px;
    height:10px;
    border-radius:4px;
    border:1px solid #B5BBBF;
    display:none;
}
::-webkit-scrollbar-button:hover{
    border-color:#5F6366;
}
::-webkit-scrollbar-button:vertical:start:increment,
::-webkit-scrollbar-button:vertical:end:decrement {
    display:none;
}
/* 标准浏览器下面无间距 */
#keepLogin,iframe[id^="_xsend_frm__xhrInstence_"]{
    border:none;
    display:none;
}

/* 遮罩 */
.ui-mask{
    position:fixed;
    _position:absolute;
    left:0;
    top:0;
    z-index:998;
    width:100%;
    height:100%;
    background-color:#000;
    opacity:.65;
    filter:alpha(opacity=65);
}
.ui-mask-white{background:#F1F1F1;}

.icon-loading {
    width: 16px;
    height: 16px;
    background: url(http://imgcache.qq.com/vipstyle/nr/box/web/images/loading3.gif) 0 0 no-repeat;
    display: inline-block;
    _display: inline;
    _zoom: 1;
}

.ui-qqlogin {
    position: fixed;
    left: 50%;
    top: 50%;
    width: 490px;
    height: 328px;
    margin-left: -246px;
    margin-top: -165px;
    display: block;
    z-index: 3000;
    background-color: white;
}
.zeroclipboard-is-hover{
    cursor: pointer !important;
}
/* 用于IE下强制DOM重绘，by cluezhang */
.x-repaint {
    zoom: 1;
    background-color: transparent;
    -moz-outline: none;
    outline: none;
}

/* 小改版 s */
.header-cnt{
	*z-index:1;
}
.header-right{
	position:absolute;
	right:-5px;
	top:0;
	z-index:11;
}
.header-right .user-info{display:block;}
/* 用户头像 */
.user-avatar{
	display:block;
	width:70px;
    padding:0 5px;
	height:50px;
}
.user-avatar .ui-arr{
	width:16px;
	height:16px;
	display:inline-block;
	vertical-align:middle;
	background:url(../img/disk-spr.png) -105px -171px no-repeat;
	cursor:pointer;/* IE6 */
	_background-image:url(../img/disk-spr-ie6.png);
}
.user-avatar img{
	margin:9px;
	margin-right:2px;
	width:32px;
	height:32px;
	vertical-align:middle;
	cursor:default;
}
.user-avatar-hover{
	background:#fff url(../img/avatar-hover-bg.png?t=1) no-repeat left top;
}
/* 用户浮层 */
.ui-pop-user{
	width:168px;
	right:0;
	top:50px;
	background:#FFF;
	border-color:#abb0bb;
	border-top:none;
	border-radius:0;
	color:#666;
	z-index:10;
}
.ui-pop-user .ui-pop-head{
	padding:0 15px;
}
.ui-pop-user .user-nick{
    display: inline-block;
    width: 138px;
    line-height:38px;
    color:#333333;
    white-space: nowrap;
    overflow: hidden;
    text-overflow:ellipsis;/* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
    -ms-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;/* Opera */
    -moz-binding: url("ellipsis.xml#ellipsis");/*FireFox*/
}
.ui-pop-user .quota-info{
	margin-bottom:5px;
}
.ui-pop-user .ui-text{
	color:#b2b2b2;
}
.ui-pop-user .ui-arr{display:none;}
.ui-pop-user .ui-quota{
	padding:0;
	border-color:#d8d8d8;
	background-color:#f2f2f2;
}
.ui-pop-user .ui-quota-bar{
	margin:-1px;
	border:1px solid #7ab737;
	
	background-color:#abe769;
	background-image:-webkit-gradient(linear, left top, left bottom, from(#abe769), to(#9bdd52));
	background-image:-moz-linear-gradient(top, #abe769, #9bdd52);
	background-image:-o-linear-gradient(top, #abe769, #9bdd52);
	
	background-image:-webkit-linear-gradient(#abe769 0%, #9bdd52 100%);
	background-image:linear-gradient(#abe769, #9bdd52);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#abe769', EndColorStr='#9bdd52');
	_position:relative;
}
.ui-pop-user .full .ui-quota-bar{
	border-color:#a52d06;
	background-color:#BD3A0F;
	background-image:-webkit-gradient(linear, left top, left bottom, from(#D65931), to(#BD3A0F));
	background-image:-webkit-linear-gradient(top, #D65931, #BD3A0F);
	background-image:-moz-linear-gradient(top, #D65931, #BD3A0F);
	background-image:-o-linear-gradient(top, #D65931, #BD3A0F);
	background-image:linear-gradient(#D65931, #BD3A0F);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#D65931', EndColorStr='#BD3A0F');
}
.ui-pop-user .ui-menu{
	margin-top:10px;
	padding:0 1px 1px;
	border-top:1px solid #E6E6E6;
	zoom:1;
}
.ui-pop-user .ui-menu a{
	position:relative;
	display:block;
	padding:7px 3px 7px 34px;
	color:#333333;
	vertical-align:bottom;
	*zoom:1;
	*background:url(about:blank);
}
.ui-pop-user .ui-menu a:hover{
	background-color:#EAEAEA;
}
.ui-pop-user .ui-menu i{
	position:absolute;
	left:10px;
	top:50%;
	display:block;
	margin-top:-8px;
	width:16px;
	height:16px;
}

.ui-pop-user .ui-menu .menu-text{
	color:#7d838f;
}
.ui-pop-user .icon-pwd,
.ui-pop-user .icon-pwd-unlock,
.ui-pop-user .icon-dwn,
.ui-pop-user .icon-fedbk,
.ui-pop-user .icon-exit{
	background:url(../img/disk-spr.png) -999px -999px no-repeat;
	_background-image:url(../img/disk-spr-ie6.png);
}
.ui-pop-user .icon-pwd{background-position:-1px -171px;}
.ui-pop-user .icon-pwd-unlock{background-position:-84px -171px;}
.ui-pop-user .icon-dwn{background-position:-22px -171px;}
.ui-pop-user .icon-fedbk{background-position:-43px -171px;}
.ui-pop-user .icon-exit{background-position:-63px -171px;}

/* appbox 小改版部分 */
.app-appbox .nav-pos{z-index:10;}
.app-appbox .header-right{right:5px;}
.app-appbox .user-avatar{
	padding-left:14px;
	padding-right:8px;
	width:auto;
	height:34px;
	font:12px/30px '\5B8B\4F531','\5FAE\8F6F\96C5\9ED1','\534E\6587\7EC6\9ED1';
	color:#FFF;
	_float:left;
}
.user-info-hover .user-avatar{
    border-left: solid 1px #78a9c1;
    border-right: solid 1px #78a9c1;
}
.app-appbox .user-nickname{
	display:inline-block;
	vertical-align:middle;
	max-width:126px;
	overflow:hidden;
	-o-text-overflow:ellipsis;
	text-overflow:ellipsis;
	white-space:nowrap;
	word-wrap:normal;
	cursor:pointer;/* IE7- */
}
.app-appbox .user-avatar .ui-arr{background-position:-126px -171px;_font-size:10px;}
.app-appbox .ui-pop-head{padding-top:7px;}
.app-appbox .ui-pop-user{display:none;top:34px;right:1px;}
.user-info-hover .ui-pop-user{display:block;}
.app-appbox .user-info-hover .user-avatar{
	color:#333;
	background:#FFF;
}
.app-appbox .user-info-hover .user-avatar .ui-arr{background-position:-105px -171px;}
